<script>
	$(document).ready(function(){

		/* 2012.11.12 - theRok - start */
		$('.li_count').hide();

		$('.btnName').click(function(){
			var userId = $(this).attr('name');
			var isMember = $(this).attr('id');
			if (document.domain == "dcinside.com")
			{
				var targetUrl = "http://gall.dcinside.com/ajax/getInfo.php";
			}
			else if (document.domain == "dcgame.in")
			{
				var targetUrl = "http://gall.dcgame.in/ajax/getInfo.php";
			}
			$('.li_count').find('strong').each(function(){
				$(this).text('');
				$(this).find('a').attr('href','');
			});
			if (isMember=="0")
			{
				$('.li_count').hide();
				$('.dot_target').removeClass('dot_line');
			}
			else
			{
				$.ajax({
					type:'GET',
					data:'user_id='+userId,
					url:targetUrl,
					dataType:'json',
					success:function(html){
						$.each(html,function(i,n){
							if (n['write_total']>0 || n['reply_total']>0)
							{
								$('.li_count').each(function(){
									if ($(this).index()==0)
									{
										$(this).find('strong').text(n['write_total']);
									}
									else if ($(this).index()==1)
									{
										$(this).find('strong').text(n['reply_total']);
									}
								});
								$('.li_count').show();
								$('.dot_target').addClass('dot_line');
								$('.li_count').each(function(){
									$(this).find('a').attr('href','http://gallog.dcinside.com/'+userId+'?type=list');
								});
							}
						});
					}
				});
			}
			/* 2012.11.12 - theRok - end */
		});
	});
</script>

