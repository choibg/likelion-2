<div id='ppd_GT' style="position:absolute;top:199px;left:130px;display:none;"></div>
<script>
	function getOffsetPpdTop(o) {
		return o ? o.offsetTop + getOffsetPpdTop(o.offsetParent) : 0;
	} 

	function getOffsetPpdLeft(o) {
		return o ? o.offsetLeft + getOffsetPpdLeft(o.offsetParent) : 0; 
	}

	function ppdImgAllMenu(){
		var offsetLeft = getOffsetPpdLeft(document.getElementById("ShowSearchTd"));
		var offsetTop = getOffsetPpdTop(document.getElementById("ShowSearchTd"));

		document.getElementById('ppd_GT').style.left = (offsetLeft+7) +'px';

		if(isIE == true){
			if(0) document.getElementById('ppd_GT').style.top  = offsetTop - 3;
			else document.getElementById('ppd_GT').style.top  = offsetTop - 3;
		}

		GEID('ppd_GT').innerHTML = "<iframe src='http://gall.dcinside.com/ppdImg/ppdImgAll.php?id=tree&sGubun=/list.php' frameborder=no width='358' height='231' allowTransparency = 'true'></iframe>"; 
		GEID('ppd_GT').style.display='';
	}

	function ppdImgAllMenu_new(){
		
		document.getElementById('ppd_GT').style.left = '107px';
		document.getElementById('ppd_GT').style.top = '167px';

		GEID('ppd_GT').innerHTML = "<iframe src='http://gall.dcinside.com/ppdImg/ppdImgAll.php?id=tree&sGubun=/list.php' frameborder=no width='358' height='231' allowTransparency = 'true'></iframe>"; 
		GEID('ppd_GT').style.display='';
	}

	function ppdImgAllMenuClose(){
		GEID('ppd_GT').style.display='none';
	}
</script>

