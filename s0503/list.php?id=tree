		function getRealOffsetTop(o) {
			return o ? o.offsetTop + getRealOffsetTop(o.offsetParent) : 0;
		} 

		function getRealOffsetLeft(o) {
			return o ? o.offsetLeft + getRealOffsetLeft(o.offsetParent) : 0; 
		}                        

		function moreImg() {
			
			var offsetTop  = getRealOffsetTop(document.getElementById("moreImg"));
			var offsetLeft  = getRealOffsetLeft(document.getElementById("moreImg"));
				
			document.getElementById("gnb_layer2").style.top = (offsetTop - -30)  + "px";
			document.getElementById("gnb_layer2").style.left = (offsetLeft - 332)+ "px";			
		}		

