		<script>
document.domain	=	"dcinside.com";			var AJAX_CONFIG = {
				timeout : 0,
				cldMemoMaxLen : 0
			};

			var LuckView = new AjaxClass("LuckView");

			function close1() {
				parent.document.getElementById("luck_div").style.display ="none";
			}

			function getRealOffsetTop(o)
			{
				return o ? o.offsetTop + getRealOffsetTop(o.offsetParent) : 0;
			}

			function getRealOffsetLeft(o)
			{
				return o ? o.offsetLeft + getRealOffsetLeft(o.offsetParent) : 0;
			}

			function doImg1()	{
				var offsetLeft, offsetTop, offsetRight;

				if(document.getElementById("luck_div").style.display=="none") {

					LuckView.getAHAHData("./luck_div.php",function(){eval();}, '');

					offsetLeft	=	getRealOffsetLeft(document.getElementById("luck_img"));
					offsetTop	=	getRealOffsetTop(document.getElementById("luck_img"));

					if(navigator.appName != 'Netscape') {
						offsetRight = document.body.clientWidth - offsetLeft - 215;
					} else {
						offsetRight = document.body.clientWidth - offsetLeft - 200;
					}

					document.getElementById("luck_div").style.top	 = offsetTop + 25 + "px";
					document.getElementById("luck_div").style.right	 = offsetRight + "px";

					document.getElementById("listiframe").src="http://saju.dcinside.com/miniunse/popup.php";
					document.getElementById("luck_div").style.display="";

					window.setTimeout('frameauto1()', 1000);

				} else {
					document.getElementById("luck_div").style.display="none";
					document.getElementById("listiframe").src="";
				}
			}

			function frameauto1() {
				if(eval(document.getElementById('listiframe').name+ ".document.body.scrollHeight")) {
					document.getElementById('listiframe').height = eval(document.getElementById('listiframe').name+ ".document.body.scrollHeight") + "px";
					document.getElementById("luck_div").height =  eval(document.getElementById('listiframe').name+ ".document.body.scrollHeight") + "px";
				}
			}

			function frameauto2(height) {
				document.getElementById('listiframe').height = height + "px";
				document.getElementById("luck_div").height =  height + "px";
			}

		</script>
	 
	
		
<script>
	$(document).ready(function(){
		var target_table;
		var num_rand_max = 3;/* hit box count */
		var num_hitissue_now;/* now hit box number */
		var num_max_hit;
		var num_hit_cnt = 6;
		var num_hit_now = Math.ceil(Math.random()*num_hit_cnt);
		var hitissue_chk = "false";
		var randbox_chk = "false";
		var js_hit_chk = "true";
		var js_issue_chk = "true";
		var js_oldhit_chk = "false";
		var js_oldhit2_chk = "true";

		var array_table_name = new Array();
		$('#wrap_randbox').find('table').each(function(){
			if ($(this).attr('name')&&$(this).attr('show_stats'))
			{
				array_table_name.push($(this).attr('name'));
			}
		});
		var num_randbox_table_cnt = array_table_name.length;
		var num_randbox_now = (Math.ceil(Math.random()*num_randbox_table_cnt))-1;

		if (hitissue_chk == "true")
		{
			$('a.l_refresh[target_class=table_random_1]').remove();
			if (js_hit_chk=="false")
			{
				$('#hit_table').remove();
			}
			if (js_issue_chk=="false")
			{
				$('#issue_table').remove();
			}
			if (js_oldhit_chk=="false")
			{
				$('#oldhit_table').remove();
			}
			$('.table_random_1').each(function(){
				load_right_best($(this).attr('table_code'));
			});
		}
		else if (hitissue_chk == "false")
		{
			if (js_oldhit2_chk=="true") {
				num_rand_max = 2;
				$('#oldhit_table').remove();
				load_right_best(3);
			}
			num_hitissue_now = Math.ceil(Math.random()*num_rand_max);
			show_table_with_num('table_random_1',num_hitissue_now);
		}

		$('.l_refresh').click(function(event){
			var class_target = $(this).attr('target_class');
			var index_target = $(this).attr('sort_code');
			var next_target = 0;
			if (index_target == num_rand_max)
			{
				next_target = 1;
			}
			else
			{
				next_target = parseInt(index_target) +1;
			}
			//$('.'+class_target).each(function(){
			$('div#hit_issue_box').find('table.table_random_1').each(function(){
				if ($(this).index()==next_target)
				{
					$(this).show();
					show_table_with_num(class_target,$(this).index());
				}
			});
			event.preventDefault();
		});

		$('input.pre, input.next').click(function(){
			var tar_code = $(this).attr('target_code');
			var tar_class = "";
			var tar_child = "";
			if (tar_code == "hit")
			{
				tar_class = "hit_article";
				tar_child = "a";
			}
			if (tar_code == "issue")
			{
				tar_class = "user_article";
				tar_child = "ul";
			}
			if (tar_code == "oldhit")
			{
				tar_class = "old_article";
				tar_child = "a";
			}
			if (tar_code == "interview") {
				tar_class = "dc_interview";
				tar_child = "a";
			}
			if ($(this).attr('class')=="pre")
			{
				$('.'+tar_class).find(tar_child).each(function(){
					if ($(this).css('display')=="block")
					{
						$(this).css({'display':'none'});
						if ($(this).is(':first-child'))
						{
							$('.'+tar_class+' '+tar_child+':last-child').css({'display':'block'});
						}
						else
						{
							$('.'+tar_class+' '+tar_child+':eq('+($(this).index()-1)+')').css({'display':'block'});
						}
						return false;
					}
				});
			}
			else if ($(this).attr('class')=="next")
			{
				$('.'+tar_class).find(tar_child).each(function(){
					if ($(this).css('display')=="block")
					{
						$(this).css({'display':'none'});
						if ($(this).is(':last-child'))
						{
							$('.'+tar_class+' '+tar_child+':first-child').css({'display':'block'});
						}
						else
						{
							$('.'+tar_class+' '+tar_child+':eq('+($(this).index()+1)+')').css({'display':'block'});
						}
						return false;
					}
				});
			}
		});


		if (randbox_chk=="true")
		{
			$('#box_change').hide();
			$('#wrap_randbox').find('table').each(function(){
				var tmp_name = $(this).attr('name');
				$(this).show().attr('show_stats','yes');
				switch (tmp_name)
				{
					case 'right_frame_realtime':
						$(document).ready(keyword_rank);
						break;
					case 'right_frame_chart':
						$(this).find('iframe').attr('src','http://wstatic.dcinside.com/dcchart/gall_right_chart.html');
						break;
					case 'right_frame_olleh':
						$(this).find('iframe').attr('src','?referer=http://gallery.dcinside.com/rightsidebanner');
						break;
					case 'right_frame_wagle':
						break;
					case 'right_frame_interview':
						$(document).ready(interview_load);
						break;
				}
			});
		}
		else if (randbox_chk=="false")
		{
			for (var i=0; i<array_table_name.length; i++)
			{
				$('#wrap_randbox table[name='+array_table_name[i]+']').hide();
			}
			target_table = $('#wrap_randbox').find('table[name='+array_table_name[num_randbox_now]+']');
			switch (array_table_name[num_randbox_now])
			{
				case 'right_frame_realtime':
					$(document).ready(keyword_rank);
					$('.btn_randbox_ro').css({'top':'10px'});
					$('#box_change').show();
					break;
				case 'right_frame_chart':
					target_table.find('iframe').attr('src','http://wstatic.dcinside.com/dcchart/gall_right_chart.html');
					$('.btn_randbox_ro').css({'top':'12px'});
					$('#box_change').show();
					break;
				case 'right_frame_olleh':
					target_table.find('iframe').attr('src','?referer=http://gallery.dcinside.com/rightsidebanner');
					$('.btn_randbox_ro').css({'top':'13px'});
					$('#box_change').hide();
					break;
				case 'right_frame_wagle':
					$('.btn_randbox_ro').css({'top':'13px'});
					$('#box_change').show();
					break;
				case 'right_frame_interview':
					$(document).ready(interview_load);
					$('.btn_randbox_ro').css({'top':'10px'});
					$('#box_change').show();
					break;
			}
			target_table.show().attr('show_stats','yes');
		}

		

		$('.btn_randbox_ro').click(function(){
			var num_randbox_max = num_randbox_table_cnt-1;

			if (num_randbox_now==num_randbox_max)
			{
				num_randbox_now = 0;
			}
			else
			{
				num_randbox_now = num_randbox_now + 1;
			}

			target_table = $('#wrap_randbox table[name='+array_table_name[num_randbox_now]+']');
			target_table.show().siblings().hide();

			switch (array_table_name[num_randbox_now])
			{
				case 'right_frame_realtime':
					if ($('#realtime_word_1').text().length<1)
					{
						$(document).ready(keyword_rank);
					}
					$('.btn_randbox_ro').css({'top':'10px'});
					$('#box_change').show();
					break;
				case 'right_frame_chart':
					if (!target_table.find('iframe').attr('src'))
					{
						target_table.find('iframe').attr('src','http://wstatic.dcinside.com/dcchart/gall_right_chart.html');
					}
					$('.btn_randbox_ro').css({'top':'12px'});
					$('#box_change').show();
					break;
				case 'right_frame_olleh':
					if (!target_table.find('iframe').attr('src'))
					{
						target_table.find('iframe').attr('src','?referer=http://gallery.dcinside.com/rightsidebanner');
					}
					$('.btn_randbox_ro').css({'top':'13px'});
					$('#box_change').hide();
					break;
				case 'right_frame_wagle':
					$('.btn_randbox_ro').css({'top':'13px'});
					$('#box_change').show();
					break;
				case 'right_frame_interview':
					if ($('.dc_interview').text().length<1)
					{
						$(document).ready(interview_load);
					}
					$('.btn_randbox_ro').css({'top':'10px'});
					$('#box_change').show();
					break;
			}
		});

		$('.pre_realtime').click(function(){
			rank_pre_show();
		});
		$('.next_realtime').click(function(){
			rank_pre_show();
		});

		$('.hit_log').live('click',function(event){
			$.ajax({
				data:"table="+$(this).attr('log_code'),
				type:"GET",
				url:"http://gall.dcinside.com/log/rightframe_hit.php",
				success:function(html){
					if (html=="1")
					{
						return true;
					}
				}
			});
		});

		/* recruit - start */
		var cookie_value = get_cookie('recruit');
		if (cookie_value=="ok")
		{
			$('#recruit_tr').hide();
		}
		$('.recruit_btn_1').click(function(){
			if (cookie_value!="ok")
			{
				set_cookie('recruit', 'ok', '8760', 'gall.dcinside.com');
				$('#recruit_tr').hide();
			}
		});
		/* recruit - end */


		function load_right_best(no) {
			if (no==1)
			{
				var jqxhr = $.getJSON('http://json.dcinside.com/hit_gall.php?jsoncallback=?', function() {

				})
				.success(function() {
					$.getJSON('http://json.dcinside.com/hit_gall.php?jsoncallback=?', function(data) {
						$.each(data,function(i,n){
							var tmp_i = (i+1);
							var tmp_display = "none";
							if (tmp_i<=num_hit_cnt)
							{
								if (tmp_i == num_hit_now)
								{
									tmp_display = "block";
								}
								$('.hit_article').append('<a class="prv hit_log" href="'+n['link']+'" style="display:'+tmp_display+';" target="_blank" log_code="hit"><img src="'+n['original_link']+'" width="166" height="119"/><span class="ellipsis">'+n['title']+'</span></a>');
							}
						});
					});
				})
				.error(function() {
				});
			}
			else if (no==2)
			{
				var jqxhr = $.getJSON('http://json.dcinside.com/issue.php?jsoncallback=?', function() {

				})
				.success(function() {
					$.getJSON('http://json.dcinside.com/issue.php?jsoncallback=?', function(data) {
						var loop = 0;
						var loop2 = 1;
						var tmp_str = "";

						var arrLink = new Array();
						var arrCategory = new Array();
						var arrTitle = new Array();
						var arrImage_link = new Array();

						$.each(data,function(i,n){
							arrLink[i] = n['link'];
							arrCategory[i] = n['category'];
							arrTitle[i] = n['title'];
							arrImage_link[i] = n['image_link'];
							loop++;
						});

						var num_issue_cnt = loop;
						var num_issue_now = Math.ceil(Math.random()*(num_issue_cnt/2));

						for (var i=0; i<arrLink.length; i++) {
							var num_chk = (i+1)%2;
							var tmp_display = "none";
							if (loop2==num_issue_now)
							{
								tmp_display = "block";
							}
							if (num_chk==1)
							{
								tmp_str = "<ul issue_code='"+loop2+"' style='display:"+tmp_display+";'>";
							}
							tmp_str += "<li><a class='hit_log' href='"+arrLink[i]+"' title='["+arrCategory[i]+"] "+arrTitle[i]+"' target='_blank' log_code='issue'><img src='"+arrImage_link[i]+"' width='80' height='80'/><strong class='ellipsis'>["+arrCategory[i]+"]</strong><span class='ellipsis'>"+arrTitle[i]+"</span></a></li>";
							if (num_chk==0)
							{
								tmp_str += "</ul>";
								$('.user_article').append(tmp_str);
								loop2++;
							}
						}
					});
				})
				.error(function() {
				});
			}
			else if (no==3)
			{
				var jqxhr = $.getJSON('http://json.dcinside.com/oldhit.php?jsoncallback=?', function() {

				})
				.success(function() {
					$.getJSON('http://json.dcinside.com/oldhit.php?jsoncallback=?', function(data) {
						var arrLink = new Array;
						var arrLink2 = new Array;
						var arrGapDate = new Array;
						var arrTitle = new Array;
						var arrRegDate = new Array;
						var totalCnt = 0;

						$.each(data,function(i,n){

							arrLink[i] = n['link'];
							arrLink2[i] = n['original_link'];
							arrGapDate[i] = n['gap_date'];
							arrTitle[i] = n['title'];
							arrRegDate[i] = n['reg_date'];
							totalCnt++;
						});

						var oldhit_now = Math.ceil(Math.random()*totalCnt);

						for (var i=0; i<totalCnt; i++) {
							var tmp_i = (i+1);
							var tmp_display = "none";
							
							if (tmp_i == oldhit_now)
							{
								tmp_display = "block";
							}
							$('.old_article').append('<a class="prv hit_log old_area" href="'+arrLink[i]+'" style="display:'+tmp_display+';" target="_blank" log_code="oldhit"><img src="'+arrLink2[i]+'" width="166" height="119"/><span class="old_img"><img src="http://nstatic.dcinside.com/dgn/oldhit/oldhit_'+arrGapDate[i]+'.gif" alt=""/></span><div style="cursor:pointer;">'+arrTitle[i]+'<span><strong>작성일</strong>'+arrRegDate[i]+'</span></div></a>');
						}
					});
				})
				.error(function() {
					//alert('error');
				});
			}
		}

		function show_table_with_num (table,no) {
			$('.'+table).each(function(){
				if ($(this).index()==no)
				{
					$(this).show().siblings().hide();
					if ($(this).find('.contents').html().length<1)
					{
						load_right_best(no);
					}
				}
			});
		}
	});
</script>

