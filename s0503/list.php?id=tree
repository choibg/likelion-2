	function setCookie( name, value, expiredays ) { 
    var todayDate = new Date(); 
        todayDate.setDate( todayDate.getDate() + expiredays ); 
        document.cookie = name + "=" + escape( value ) + "; path=/; ;" 
    } 

	function setCookie_hk( name, value, expiredays ) { 
    domain = ".dcinside.com";
	var todayDate = new Date();
	todayDate.setDate( todayDate.getDate() + expiredays );
	document.cookie = name + "=" + escape( value ) + "; path=/; expires=" + todayDate.toGMTString() + ((domain) ? "; domain=" + domain : "") +";";
    } 
	
	function gallSearchMenu(id){
		GEID('SearchIframe').src='leftIfrm_new_080522.php?id='+id;
