		function recommend_AJAX(URL,query)
			{
	 
			 var xmlhttp = null;
			 try {
			  xmlhttp = new XMLHttpRequest();
			 } catch (trymicrosoft) {
			  try {
			   xmlhttp = new ActiveXObject("Msxml2.XMLHTTP");
			  } catch (othermicrosoft) {
			   try {
				xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
			   } catch (failed) {
				xmlhttp = false;
			   } 
			  }
			 }
			with(xmlhttp) {
			
				 open("POST", URL, true);
				 setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
				 onreadystatechange = function(){
				  if(readyState == 4 && status == 200){
					var result = responseText;
					if(result==1){
						//alert('추천 하였습니다.');
						document.getElementById('recomm_btn').innerHTML++;
					}
					else if(result==3){
						alert('실명인증 또는 로그인 해주시기 바랍니다.');
					}
					else{
					
						alert('추천은 한 게시물당 \n한번만 하실수 있습니다.');
					}
				  }
				}
				send(query);
			}
			
			}

