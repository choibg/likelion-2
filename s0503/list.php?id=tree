	function closeSearchMenu(){
		if(GEID('SearchIframe')) {
			GEID('SearchIframe').src='';	
		}
		if(GEID('gallSearchMenu')) {
			GEID('gallSearchMenu').style.display='none';
		}
		if(GEID('ShowSearchImg')) {
			GEID('ShowSearchImg').style.display='inline';
		}
		deleteCookie("gallSearchMenu");
		deleteCookie("goURL");
		deleteCookie("str");
		deleteCookie("kinds");
		
	}
	

