<script>
	var check_search = function (check_division) {

		if (check_division == '1') {
			document.getElementById("qrobo_search_select").style.display = 'inline'
			document.getElementById("search_select").style.display = 'none'
		} else {
			document.getElementById("qrobo_search_select").style.display = 'none'
			document.getElementById("search_select").style.display = 'inline'
		}

	}

	var check_value = function () {

		division_check = document.getElementById('check_search_1').checked;
	
		//큐로보 검색
		if (division_check == '1') {
			search_type = document.getElementById('qrobo_search_select').value;

			document.qrobo_search.q.value= document.getElementById('search_word').value;

			if (search_type == '1') {
				document.qrobo_search.g_s1.value = 1;
				document.qrobo_search.g_s2.value = "";
				document.qrobo_search.g_s3.value = "";
			} else if (search_type == '2') {
				document.qrobo_search.g_s1.value = "";
				document.qrobo_search.g_s2.value = 1;
				document.qrobo_search.g_s3.value = "";
			} else if (search_type == '3') {
				document.qrobo_search.g_s1.value = "";
				document.qrobo_search.g_s2.value = "";
				document.qrobo_search.g_s3.value = 1;
			} else if (search_type == '4') {
				document.qrobo_search.g_s1.value = "";
				document.qrobo_search.g_s2.value = 1;
				document.qrobo_search.g_s3.value = 1;
			}
			document.qrobo_search.submit();
		} else {
			search_type = document.getElementById('search_select').value;

			document.search.keyword.value= document.getElementById('search_word').value;
			document.search.k_type.value= document.getElementById('search_select').value;
			document.search.submit();
		}	
		
	}
</script>
