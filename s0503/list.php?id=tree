			<script>
		function service() {
			if(document.getElementById("category").style.display =="") {
				document.getElementById("service_f").innerHTML ="<img src='http://nstatic.dcinside.com/gn/images/list_spread.gif' alt='서비스펴기'/>  <a class='adlink' onclick='javascript:service();' style='cursor:pointer;'>서비스펴기</a>";
				document.getElementById("category").style.display="none";
				service_cookie();

			} else if(document.getElementById("category").style.display =="none") {
				document.getElementById("service_f").innerHTML ="<img src='http://nstatic.dcinside.com/gn/images/list_fold.gif' alt='서비스접기'/>  <a class='adlink' onclick='javascript:service();' style='cursor:pointer;'>서비스접기</a>";
				document.getElementById("category").style.display="";
				service_cookie_cancel();
			}
		}

		function day_setCookie(adname, advalue, expiredays) {
             
			var todayday = new Date();
			todayday.setDate( todayday.getDate() + expiredays );
			document.cookie = adname + "=" + escape( advalue ) + "; path=/; expires=" + todayday.toGMTString() + ";"
		}

		function service_cookie() {
			day_setCookie("service_cookie", "done" , 1 );
		}

		function service_cookie_cancel() {
			day_setCookie("service_cookie", "candel" , 1 );
		}

		function scrollTop1() {
			if(navigator.appVersion.indexOf('Safari') != -1) {
				document.body.scrollTop =0;
			} else {
				document.documentElement.scrollTop =0;
			}
		}

       function mobile_version(no) {
         domain = ".dcinside.com";
	     var cookieName = "m_dcinside_web";
         var expireDate = new Date();
         
           expireDate.setDate( expireDate.getDate() - 0 );
          document.cookie = cookieName + "= " + "; path=/; expires=" + expireDate.toGMTString() + ((domain) ? "; domain=" + domain : "") +";";

            var no;  
            if(no){
            location.href="http://m.dcinside.com/view.php?id=tree&no="+no;
            }else{
			 location.href="http://m.dcinside.com/list.php?id=tree";
            }
		}

