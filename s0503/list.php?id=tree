<!-- 구글광고 시작>
	<tr id="listGoogleAd" name="listGoogleAd" style="display:block">
		<td>
			<script language="JavaScript">
				function google_afs_request_done(google_ads) {
					var s = '';
					var i;

					if (google_ads.length == 0) {
					  return;
					}

					s	=	"<table border=0 cellspacing=0 cellpadding=0 width='890px' style='padding:0px 5px 0px 30px;'><tr><td style='line-height:30px'>";
					for (var key in google_ads) {

						s	+=	'<strong><a href="'+ google_ads[key].url +'" target="_blank">'+ google_ads[key].line1 +'</a></strong> -  <a href="'+ google_ads[key].url +'" target="_blank">'+ google_ads[key].line2 + google_ads[key].line3 +'</a> <a href="'+ google_ads[key].url +'" target="_blank"><span class="url">'+ google_ads[key].visible_url +'</span></a><br>'
							+		'';
					}
