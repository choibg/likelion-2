	function goThere(){
        if(!document.theForm.theMenu.selectedIndex==""){
		location.href=document.theForm.theMenu.options[document.theForm.theMenu.selectedIndex].value;}
	}

	function step1(){
		document.theForm.selval.value++;
		step2();
	}

	function step2(){
		if(document.theForm.selval.value >= 2 && document.theForm.theMenu.selectedIndex==""){
			document.theForm.selval.value = 0;
			location.href='http://gall.dcinside.com/list.php?id=top_f_drama';
		}
	}


	/* sa 추가 (해외축구 리플 보기)*/
	function newXMLHttpRequest_rComment() {
		var xmlreq_r = false;
		if (window.XMLHttpRequest) { // 파이어폭스나 맥의 사파리의 경우처리
			// Create XMLHttpRequest object in non-Microsoft browsers
			xmlreq_r = new XMLHttpRequest();
		}
		else if (window.ActiveXObject) {    // IE계열의 브라우져의 경우
			// Create XMLHttpRequest via MS ActiveX
			try {
				xmlreq_r = new ActiveXObject("Msxml2.XMLHTTP");
			}catch (e1) {
				// Failed to create required ActiveXObject
				try {
					xmlreq_r = new ActiveXObject("Microsoft.XMLHTTP");
				}
				catch (e2) {
					// Unable to create an XMLHttpRequest with ActiveX
				}
			}
		}
		return xmlreq_r;
	}

	function processReqChange_rComment() {
		// only if req shows "loaded"
		if (r_Req.readyState == 4) {
			// only if "OK"
			if (r_Req.status == 200) {
				setComment(r_Req);
			}else {
				// alert("There was a problem retrieving the XML data:\n" + g_Req.statusText);
			}
		}
	}

	function getCommentData(id, no) {

		r_Req = newXMLHttpRequest_rComment();
		r_Req.onreadystatechange = processReqChange_rComment;
		r_Req.open("POST", "./_realComment.php", true);
		r_Req.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
		r_Req.send("id="+id+"&no="+no);

	}

	function setComment(rReq) {
	   if(GEID("rad2")) {
		var x = GEID("rad2");
		inner_data = rReq.responseText;

		x.innerHTML = inner_data;
	   }
	}
	/* sa 추가 (해외축구 리플 보기)*/

	function ticker(){
		getCommentData('tree','4130009')
	}
		
